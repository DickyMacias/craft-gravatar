{
    "name": "dickymacias/gravatar",
    "description": "Adds gravatar support",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "gravatar"
    ],
    "support": {
        "docs": "https://github.com/DickyMacias/gravatar/blob/master/README.md",
        "issues": "https://github.com/DickyMacias/gravatar/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dicky Macias",
            "homepage": "https://github.com/DickyMacias"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0|^5.0.0",
        "php": "^8.0.2"
    },
    "autoload": {
        "psr-4": {
          "dickymacias\\gravatar\\": "src/"
        }
    },
    "extra": {
        "name": "Gravatar",
        "handle": "gravatar",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/DickyMacias/gravatar/master/CHANGELOG.md",
        "components": {
            "url": "dickymacias\\gravatar\\services\\Url",
            "img": "dickymacias\\gravatar\\services\\Img"
        },
        "class": "dickymacias\\gravatar\\Gravatar"
    }
}
